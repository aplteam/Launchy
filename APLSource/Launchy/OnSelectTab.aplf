 OnSelectTab y;ref;n;flag
 ref←1⊃,y
 n←GetRef2n ref
 :If n.GeneralTab≡ref
     flag←((n.List.SelItems⍳1)⊃n.List.∆Orig)∨.≠' '
     n.ok.Active←flag∧0<≢G.AplVersions
     n.ok.Tip←n.ok.∆Tip1
 :Else
     flag←((n.List2.SelItems⍳1)⊃n.List2.∆Orig)∨.≠' '
     n.ok.Active←flag∧0<≢G.LinuxAplVersions
     n.ok.Tip←n.ok.∆Tip2
 :EndIf
⍝Done
